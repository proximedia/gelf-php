{
    "name": "proximedia/gelf-php",
    "description": "A php implementation to send log-messages to a GELF compatible backend like Graylog2. Added TCP support.",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Zikarsky",
            "email": "benjamin@zikarsky.de"
        },
        {
            "name": "Ahmed Trabelsi",
            "email": "ahmed.trabelsi@proximedia.fr"
        }
    ],
    "require": {
        "php": ">=5.3",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Gelf\\": "src/Gelf"
        }
    },
    "provide": {
        "psr/log-implementation": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
